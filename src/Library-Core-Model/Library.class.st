"A library"Class {	#name : #Library,	#superclass : #Object,	#instVars : [		'items'	],	#classInstVars : [		'default'	],	#category : #'Library-Core-Model-Base'}{ #category : #accessing }Library class >> default [	"Return the Default of the receiver."	^default ifNil: [ default := self new ]]{ #category : #accessing }Library class >> default: aLibrary [	"Sets the Default of the receiver to the given Library."	default := aLibrary]{ #category : #accessing }Library >> items: aCollectionOfItems [	"Sets the Items of the receiver to the given collection"	items := aCollectionOfItems]{ #category : #accessing }Library >> items [	"Return the Items of the receiver."	^items ifNil: [ items := OrderedCollection new ]]